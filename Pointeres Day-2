/*Swap() Numbers with Pointers*/
#include<stdio.h>
void swap(int*	,int*	);
int main()
{
int number1,number2;
printf("Give me a number:");
scanf("%d	%d",&number1,&number2);
swap(&number1,&number2);
printf("%d	%d\n",number1,number2);
return 0;
}
void swap(int *number1,int *number2)
{
int temp;
temp=*number1;
*number1=*number2;
*number2=temp;
}

//output
Give me a number:
12  34
34	12
